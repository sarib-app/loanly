{
  "expo": {
    "name": "Suvhida",
    "slug": "loanly",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.subhidaapp.app",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "This app requires you to allow photos to use them for verification and other reasons.",
          "cameraPermission": "Allow Suvhida to access your camera."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Subhida to get your contacts otherwise you wont be able to take loans."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7665d305-4207-485a-afb6-42ffaa4c2ecc"
      }
    }
  }
}
